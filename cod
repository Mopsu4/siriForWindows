                System.Diagnostics.Process.Start("https://www.google.com/search?safe=active&ei=JQqFXNfJHvvQmwX4mreACQ&q=%D0%9F%D0%B5%D1%80%D0%B5%D0%B2%D0%BE%D0%B4%D1%87%D0%B8%D0%BA&oq=%D0%9F%D0%B5%D1%80%D0%B5%D0%B2%D0%BE%D0%B4%D1%87%D0%B8%D0%BA&gs_l=psy-ab.3..0i131i67j0i67j0l8.18853.19184..19738...0.0..0.278.466.0j1j1......0....1..gws-wiz.......0i71j0i10i67j0i7i10i30j0i10.QtsfJOTanP0");
                l2.Text = Convert.ToString("Вот всё по этому запросу!");
            }
            if (l1.Text == r2)
            {
                System.Diagnostics.Process.Start("https://www.google.com/search?q=%D0%A4%D0%BE%D1%82%D0%BE%D1%88%D0%BE%D0%BF+%D0%BE%D0%BD%D0%BB%D0%B0%D0%B9%D0%BD&oq=%D1%84%D0%BE%D1%82%D0%BE&aqs=chrome.2.69i57j69i61j69i59j69i61j69i59l2.2267j0j4&sourceid=chrome&ie=UTF-8");
                l2.Text = Convert.ToString("Вот всё по этому запросу!");
            }
            if (l1.Text == r3)
            {
                System.Diagnostics.Process.Start("https://vk.com/");
                l2.Text = Convert.ToString("Вот всё по этому запросу!");
            }
            if (l1.Text == r4)
            {

                Form6 frm = new Form6();
                frm.Show();
               
            }
            if (l1.Text == r5)
            {
                System.Diagnostics.Process.Start("https://www.gismeteo.ua/catalog/belarus/");
                l2.Text = Convert.ToString("Вот всё по этому запросу!");
            }
        }

        private void Form2_Load(object sender, EventArgs e)
        {
            notifyIcon1.BalloonTipTitle = "ИМЯ";
            notifyIcon1.BalloonTipText = "СДЕЛАНО";
            notifyIcon1.Text = "ИМЯ";

            l2 = label2;
            string s = DateTime.Now.ToString("dd MMMM yyyy | HH:mm:ss");
            l1 = label1;

            System.Globalization.CultureInfo ci = new System.Globalization.CultureInfo("ru-ru");
            SpeechRecognitionEngine sre = new SpeechRecognitionEngine(ci);
            sre.SetInputToDefaultAudioDevice();

            sre.SpeechRecognized += new EventHandler<SpeechRecognizedEventArgs>(sre_SpeechRecognized);
            Choices word = new Choices();

            word.Add(new string[] { "Здраствуй", "Выйди из программы!", "Майкрософт", "Время?", "Калькулятор!", "Блокнот!", "Браузер!" 
            ,"Фейсбук","Хочу рисовать!","Яндекс","Википедия","Шутка","Твитер","Записка","Джум!","Алиекспрес!","Электронная почта!","Карта!"
            ,"АвтоРио!","Любишь мечтать?","Кто ты?!","АвтоРу!","Выйди!","Реддит!","Инстаграм!","Ютуб!","Смотреть фильмы!","Смотреть сериалы!"
            ,"Смотреть мультфильмы","Смотреть клипы!","Слушать музыку!","Смотреть аниме!","Слушать русскую музыку!","Слушать зарубежную музыку!"
            ,"Фильмы!","Сериалы!","Мультфильмы!","Аниме!","Русская музыка","Зарубежная музыка!","Онлайн игры!","Калькулятор валют!"
            ,"Погода в России","Погода в Украине!","Погода в Белоруссии!","Переводчик!","Фотошоп онлайн!","ВКонтакте!","Таймер!"
            });

            GrammarBuilder gb = new GrammarBuilder();
            gb.Append(word);
            Grammar g = new Grammar(gb);
            sre.LoadGrammar(g);
            sre.RecognizeAsync(RecognizeMode.Multiple);
        }
